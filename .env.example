# ===========================================
# CONFIGURACIÓN DE LA API DE LAUDUS
# ===========================================
# IMPORTANTE: No subir este archivo a Git
# Copia este archivo como .env y completa con tus credenciales reales

# URL de la API de Laudus
LAUDUS_API_URL=https://api.laudus.cl

# Credenciales de autenticación
LAUDUS_USERNAME=API
LAUDUS_PASSWORD=api123
LAUDUS_COMPANY_VAT_ID=77548834-4

# Configuración de timeout (milisegundos)
API_TIMEOUT=30000

# Número de reintentos en caso de fallo
API_RETRIES=3

# Cache de tokens
ENABLE_CACHE=true
CACHE_TTL=300

# Nivel de logging (debug, info, warn, error)
LOG_LEVEL=info
LOG_PRETTY=true
